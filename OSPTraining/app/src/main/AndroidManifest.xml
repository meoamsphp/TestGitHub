<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="appewtc.masterung.osptraining" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_build"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".HelpMeActivity"
            android:label="@string/title_activity_help_me"
            android:screenOrientation="portrait" />
        <activity
            android:name=".OfficerListView"
            android:label="@string/title_activity_officer_list_view"
            android:screenOrientation="portrait" />
        <activity
            android:name=".DetailActivity"
            android:label="@string/title_activity_detail"
            android:screenOrientation="portrait" />
        <activity
            android:name=".SearchOfficerActivity"
            android:label="@string/title_activity_search_officer"
            android:screenOrientation="portrait" />
        <activity
            android:name=".VideoActivity"
            android:label="@string/title_activity_video"
            android:screenOrientation="landscape" />
        <activity
            android:name=".MyMapActivity"
            android:label="@string/title_activity_my_map" />

        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyCPb7KJAaI8ijUhc23x7wC__qTjRwraxJ8" />

        <activity
            android:name=".FormPositionActivity"
            android:label="@string/title_activity_form_position" >
        </activity>
    </application>

</manifest>
